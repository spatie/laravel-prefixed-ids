{
    "name": "spatie/laravel-prefixed-ids",
    "description": "Friendly prefixed IDs for Laravel models",
    "keywords": [
        "spatie",
        "laravel-prefixed-ids"
    ],
    "homepage": "https://github.com/spatie/laravel-prefixed-ids",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0|^13.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "pestphp/pest": "^3.0",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PrefixedIds\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PrefixedIds\\Tests\\": "tests",
            "Spatie\\PrefixedIds\\Tests\\Database\\Migrations\\": "tests/database/migrations"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --colors=always",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\PrefixedIds\\PrefixedIdsServiceProvider"
            ]
        }
    }
}
